<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dadosmeteorologicos.controller.tabelaRegistrosController">
    <children>
        <Button fx:id="voltarButton" layoutX="28.0" layoutY="29.0" mnemonicParsing="false" onAction="#retornarPrincipal" prefHeight="35.0" prefWidth="78.0" text="Voltar" />
        <TableView fx:id="tabelaRegistros" layoutX="6.0" layoutY="14.0" prefHeight="600" prefWidth="1000">
            <columns>
                <TableColumn fx:id="colunaId" text="ID" />
                <TableColumn fx:id="colunaCidade" text="Cidade" />
                <TableColumn fx:id="colunaEstacao" text="Estação" />
                <TableColumn fx:id="colunaData" text="Data" />
                <TableColumn fx:id="colunaHora" text="Hora" />
                <TableColumn fx:id="colunaTemperaturaMedia" text="Temperatura Média" />
                <TableColumn fx:id="colunaUmidadeMedia" text="Umidade Média" />
                <TableColumn fx:id="colunaVelVento" text="Velocidade do Vento" />
                <TableColumn fx:id="colunaDirVento" text="Direção do Vento" />
                <TableColumn fx:id="colunaChuva" text="Chuva" />
            </columns>
        </TableView>
    </children>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
</AnchorPane>
