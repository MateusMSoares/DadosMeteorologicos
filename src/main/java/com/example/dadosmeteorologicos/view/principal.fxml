        <Button fx:id="gerarRelatorio" layoutX="160.0" layoutY="315.0" mnemonicParsing="false" onAction="#tabelaRegistroController" prefHeight="45.0" prefWidth="126.0" text="Gerar relátorio" />
        <Button fx:id="valorMedio" layoutX="300.0" layoutY="315.0" mnemonicParsing="false" onAction = "#valorMedioViewController" prefHeight="45.0" prefWidth="126.0" text="Valor Médio" />
